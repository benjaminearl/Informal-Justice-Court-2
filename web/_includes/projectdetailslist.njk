<div class="project-details">
<ul class="static-content-nav">
<li class="static-content-title">The Project</li>
{% for projectDetail in projectdetailslist | reverse %}
  {% set currentProjectDetail = projectDetail.data.projectDetail %}
  <a href="{{ projectDetail.url | url }}" class="static-content-link">
  <li class="static-nav-item{% if projectDetail.url == url %} projectdetailslist-item-active{% endif %}">
    {% if currentProjectDetail.title %}{{ currentProjectDetail.title }}{% else %}<code>{{ projectDetail.url }}</code>{% endif %}
  </li></a>
{% endfor %}
</div>
