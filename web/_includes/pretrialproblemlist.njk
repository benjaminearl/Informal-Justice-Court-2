  <ul class="static-nav-item{% if pretrialProblem.url == url %} pretrialproblemlist-item-active{% endif %}">
{% for pretrialProblem in pretrialproblemlist | reverse %}
  {% set currentPretrialProblem = pretrialProblem.data.pretrialProblem %}
      <a href="{{ pretrialProblem.url | url }}" class="static-content-link">
      <li class="content-box">
      <img src="{{ currentPretrialProblem.imageUrl }}" class="thumbnail"/>
        {% if currentPretrialProblem.title %}{{ currentPretrialProblem.title }}{% else %}<code>{{ pretrialProblem.url }}</code>{% endif %}
    </li>
    </a>
{% endfor %}
  </ul>
