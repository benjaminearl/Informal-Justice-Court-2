<ol reversed class="static-nav-item{% if pretrialProblem.url == url %} pretrialproblemlist-item-active{% endif %} content-navigation">
{% for pretrialProblem in pretrialproblemlist | reverse %}
  {% set currentPretrialProblem = pretrialProblem.data.pretrialProblem %}
  <a href="{{ pretrialProblem.url | url }}" class="static-content-link">
    <li class="content-box">
      {% if currentPretrialProblem.title %}<h3 class="content-box-title">{{ currentPretrialProblem.title }}</h3>{% else %}<code>{{ pretrialProblem.url }}</code>{% endif %}
      <time class="postlist-date" datetime="{{ currentPretrialProblem.publishedAt }}">{{ currentPretrialProblem.publishedAt|readableDate }}</time>
    </li>
  </a>
{% endfor %}
</ol>
