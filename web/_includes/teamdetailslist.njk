{% for teamDetail in teamdetailslist | reverse %}
  {% set currentTeamDetail = teamDetail.data.teamDetail %}
  <div class="teamdetailslist-item{% if teamDetail.url == url %} teamdetailslist-item-active{% endif %}">
    <a href="{{ teamDetail.url | url }}" class="teamdetailslist-link">{% if currentTeamDetail.title %}{{ currentTeamDetail.title }}{% else %}<code>{{ teamDetail.url }}</code>{% endif %}</a>
  </div>
{% endfor %}
