<div class="about-us">
<ul class="static-content-nav">
<li class="static-content-title">About Us</li>
{% for teamDetail in teamdetailslist | reverse %}
  {% set currentTeamDetail = teamDetail.data.teamDetail %}
  <a href="{{ teamDetail.url | url }}" class="static-content-link">
  <li class="static-nav-item{% if teamDetail.url == url %} teamdetailslist-item-active{% endif %}">
    {% if currentTeamDetail.title %}{{ currentTeamDetail.title }}{% else %}<code>{{ teamDetail.url }}</code>{% endif %}
  </li></a>
{% endfor %}
</div>
